{
  "name": "init-me",
  "version": "1.0.1",
  "description": "init program",
  "main": "output/task/index.js",
  "types": "output/task/index.d.ts",
  "scripts": {
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**",
    "dev": "tsc",
    "d": "tsc",
    "build": "tsc",
    "prepublishOnly": "npm run prettier && npm run d && npm test",
    "test": "jest ./test/jest",
    "test:init": "jest ./test/jest/init.test.js",
    "test:install": "jest ./test/jest/install.test.js",
    "test:uninstall": "jest ./test/jest/uninstall.test.js",
    "test:link": "jest ./test/jest/link.test.js",
    "test:unlink": "jest ./test/jest/unlink.test.js",
    "test:version": "jest ./test/jest/version.test.js",
    "test:reset": "jest ./test/jest/reset.test.js",
    "test:path": "jest ./test/jest/path.test.js",
    "test:recommend": "jest ./test/jest/recommend.test.js",
    "test:case-relative-path": "jest ./test/jest/case-relative-path.test.js",
    "test:lib": "jest ./test/jest/lib.test.js"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "yyl-react"
    ],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false
      }
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "output",
    "test"
  ],
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/init-me.git"
  },
  "keywords": [
    "init"
  ],
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/init-me/issues"
  },
  "homepage": "https://github.com/jackness1208/init-me#readme",
  "preferGlobal": true,
  "engines": {
    "node": ">= 8.0.0"
  },
  "bin": {
    "init": "bin.js"
  },
  "dependencies": {
    "@types/inquirer": "8.2.4",
    "@types/node": "^18.8.3",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "commander": "^3.0.1",
    "inquirer": "^8.2.4",
    "yyl-cmd-logger": "^0.2.1",
    "yyl-fs": "0.3.3",
    "yyl-os": "^0.14.4",
    "yyl-util": "^2.3.9"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-yyl-react": "^2.0.0",
    "jest": "^29.0.3",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.1"
  }
}
